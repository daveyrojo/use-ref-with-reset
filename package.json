{
  "name": "use-ref-with-reset",
  "version": "1.0.0",
  "description": "A Vue composable for tracking and resetting reactive state with original values.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/useRefWithReset.ts --format esm,cjs --dts --out-dir dist",
    "check-types": "tsc --noEmit",
    "dev": "vitest watch",
    "test": "vitest"
  },
  "keywords": [
    "vue",
    "composables",
    "reset",
    "state",
    "vueuse"
  ],
  "author": "David Eldridge aka DaveyRojo",
  "license": "MIT",
  "peerDependencies": {
    "@vueuse/core": "^10.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "jsdom": "^26.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0",
    "vitest": "^3.1.1"
  }
}
